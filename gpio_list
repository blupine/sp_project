body_sense
: gpio.26 -> BCM.12
power
: gpio.6 -> BCM.25
servo
: gpio.1 ->  18
touch
: gpio.28 -> BCM.20
  gpio.27 -> BCM.16
  gpio.5  -> BCM.24
  gpio.4  -> BCM.23
3led
: gpio.24 -> 22 - R
  gpio.23 -> 27 - G
  gpio.22 -> 17 - B
lcd
: gpio.21 -> D4
  gpio.22
  gpio.23
  gpio.24 -> D7
  RS -> 11
  E  -> 10
